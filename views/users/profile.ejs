<h2>User Profile</h2>

<h3>Thanks for being a user, <%= user.email %>!</h3>

<ul>
  <% allMusic.forEach(allFaves => { %>

  <li><%= allFaves.artist %></li>
  <%})%>
</ul>
<form action="/users/<%= params %>/profile" method="POST">
  <input type="text" name="comment" />
  <button type="submit">Comment</button>
</form>

<ul>
  <% allComments.forEach(allCs => { %>

  <li><%= allCs.content %></li>
  <button>Delete Comment</button>
  <%})%>
</ul>
